#include <stdio.h>

int dp_knap(int vals[], int weights[], int W, int n){
	
	int dp_arr[6][11] = {0}; //hard coded because C

	for(int i = 0; i< n; i++){
		dp_arr[i][0] = 0;	
	}
	for(int j = 0; j<W; j++){
		dp_arr[0][j] = 0;
	}
	
	
	for(int i = 0; i<n; i++){
		printf("\n");
		for(int j = 0; j<W; j++){
			printf("%d", dp_arr[i][j]);
		}
	}
}

int main(){
	//			  i = {1,2,3,4,5}	
	int vals[5]		= {4,3,6,5,7};
	int weights[5]	= {2,2,3,4,5};
	int W = 10;
	int n = 5;

	dp_knap(vals, weights, W+1, n+1);

	return 0;
}
